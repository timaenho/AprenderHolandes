@model IEnumerable<AprenderHolandes.Models.MateriaCursadaEvaluacion>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Evaluaciones</h1>
<h4>@ViewData["Grupo"]</h4>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Evaluacion)
            </th>
            <th>
                @Html.Raw("Link")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Evaluacion.Titulo)
            </td>

            <td>
                <a href="@item.Evaluacion.Descripcion">Empezar </a>
            </td>
        </tr>
}
    </tbody>
</table>

@model AprenderHolandes.Models.MateriaCursadaEvaluacion

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Eliminar</h1>

<h3>Estas Seguro?</h3>
<div>
    <h4>Evaluación por grupo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.Raw("Evaluacón")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Evaluacion.Titulo)
        </dd class>
        <dt class = "col-sm-2">
            @Html.Raw("Grupo")
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MateriaCursada.Nombre)
        </dd class>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Activo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Activo)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver</a>
    </form>
</div>
